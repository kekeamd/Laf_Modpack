function ListAddToList_text(l1: texts, l2: texts):: texts:
    loop {_l2::*}:
        add loop-value to {_l1::*}
    return {_l1::*}

function if_in_list(elem: text, l: texts):: boolean:
    if size of {_l::*} == 0:
        return false
    set {_out} to false
    loop {_l::*}:
        if {_elem} == loop-value:
            set {_out} to true
            stop loop
    return {_out}